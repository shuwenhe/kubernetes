通过 kubectl 连接 Kubernetes 集群
$HOME/.kube/config

apiVersion: v1
clusters:
- cluster:
    server: https://47.94.245.195:6443
    certificate-authority-data: 
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: "kubernetes-admin"
  name: kubernetes-admin-ccbdf99b67d43401ca4efa97df9c57ca1
current-context: kubernetes-admin-ccbdf99b67d43401ca4efa97df9c57ca1
kind: Config
preferences: {}
users:
- name: "kubernetes-admin"
  user:
    client-certificate-data: 
    client-key-data: 